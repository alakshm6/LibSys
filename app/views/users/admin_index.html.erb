<p id="notice"><%= notice %></p>
<h1>Listing Users</h1>

<table>
  <thead>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th>User type</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>

      <tr>
        <td><%= user.email %></td>
        <td><%= user.name %></td>
        <td><%= user.user_type %></td>
        <td><%= link_to 'Show', user %></td>
        <% if user.user_type == "A"%>
          <%if session[:current_user_id]!=user.id%>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy',controller: "users", action: "delete_admin",id: user.id, data: { confirm: 'Are you sure?' } %></td>
        <%end%>
            <%end%>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', admin_home_path %>

